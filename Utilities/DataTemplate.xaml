<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GetOutAdminV2.ViewModels"
                    xmlns:view="clr-namespace:GetOutAdminV2.Views">
    <DataTemplate DataType="{x:Type vm:LogInViewModel}">
        <view:LogInPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ListUsersViewModel}">
        <view:ListUserPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReportViewModel}">
        <view:ReportPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SanctionsViewModel}">
        <view:SanctionsPage/>
    </DataTemplate>
</ResourceDictionary>